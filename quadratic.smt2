; symmetry breaking constraints
(define-fun plane-le (
  (xa Real) (xb Real) (xc Real) (xd Real) (xe Real) (xf Real)
  (ya Real) (yb Real) (yc Real) (yd Real) (ye Real) (yf Real))
  Bool
  (or
    (and (< xa ya))
    (and (= xa ya) (< xb yb))
    (and (= xa ya) (= xb yb) (< xc yc))
    (and (= xa ya) (= xb yb) (= xc yc) (< xd yd))
    (and (= xa ya) (= xb yb) (= xc yc) (= xd yd) (< xe ye))
    (and (= xa ya) (= xb yb) (= xc yc) (= xd yd) (= xe ye) (< xf yf))
  ))

(assert (plane-le pa pb pc pd pe pf qa qb qc qd qe qf))
